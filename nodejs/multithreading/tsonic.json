{
  "$schema": "https://tsonic.dev/schema/v1.json",
  "rootNamespace": "MyApp",
  "entryPoint": "src/App.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "app",
  "runtime": "dotnet",
  "optimize": "speed",
  "packages": [],
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "dotnet": {
    "typeRoots": [
      "node_modules/@tsonic/js-globals",
      "node_modules/@tsonic/dotnet"
    ],
    "libraries": [
      "lib/nodejs.dll"
    ]
  }
}
