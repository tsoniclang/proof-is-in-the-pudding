{
  "$schema": "https://tsonic.org/schema/v1.json",
  "rootNamespace": "MyApp",
  "entryPoint": "src/App.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "app",
  "optimize": "speed",
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "dotnet": {
    "typeRoots": [
      "node_modules/@tsonic/globals"
    ],
    "frameworkReferences": [
      "Microsoft.AspNetCore.App"
    ]
  }
}
