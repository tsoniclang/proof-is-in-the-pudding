{
  "$schema": "https://tsonic.dev/schema/v1.json",
  "rootNamespace": "AspNetCoreBlogEf",
  "namingPolicy": {
    "classes": "clr"
  },
  "entryPoint": "src/App.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "app",
  "optimize": "speed",
  "output": {
    "nativeAot": false,
    "trimmed": false,
    "singleFile": false
  },
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "dotnet": {
    "typeRoots": [
      "node_modules/@tsonic/globals"
    ],
    "frameworkReferences": [
      "Microsoft.AspNetCore.App"
    ],
    "packageReferences": [
      {
        "id": "Microsoft.EntityFrameworkCore.Sqlite",
        "version": "10.0.1"
      }
    ]
  }
}
