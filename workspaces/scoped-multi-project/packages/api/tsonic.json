{
  "$schema": "https://tsonic.org/schema/v1.json",
  "rootNamespace": "Acme.Api",
  "entryPoint": "src/App.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "api",
  "output": {
    "nativeAot": false,
    "trimmed": false,
    "singleFile": false
  },
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "references": {
    "libraries": [
      "../domain/dist/net10.0/Acme.Domain.dll"
    ]
  }
}
